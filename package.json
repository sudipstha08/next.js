    "eslint-plugin-import": "^2.22.1",
    "firebase": "^8.2.9",
    "formik": "^2.2.6",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "next": "latest",
    "react": "^16.12.0",
    "react-apexcharts": "^1.3.7",
    "react-dom": "^16.12.0",
